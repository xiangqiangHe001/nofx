# 示例环境变量配置（复制为 .env 并填写密钥后生效）

# 服务端口
API_SERVER_PORT=8081

# AI 密钥（按需填写）
DEEPSEEK_KEY=your_deepseek_key
QWEN_KEY=your_qwen_key

# 自定义 OpenAI 兼容 API（可选）
CUSTOM_API_URL=https://api.openai.com/v1
CUSTOM_API_KEY=your_custom_api_key
CUSTOM_MODEL_NAME=gpt-4o-mini

# 统一代理（与 okx.py 保持一致）
HTTP_PROXY=http://127.0.0.1:7897
HTTPS_PROXY=http://127.0.0.1:7897

# Binance（按需）
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET_KEY=your_binance_secret_key

# Hyperliquid（按需）
HYPERLIQUID_PRIVATE_KEY=your_hyperliquid_private_key
HYPERLIQUID_TESTNET=false

# Aster（按需）
ASTER_USER=your_aster_main_wallet
ASTER_SIGNER=your_aster_api_wallet
ASTER_PRIVATE_KEY=your_aster_api_private_key

# OKX（按需）
OKX_API_KEY=your_okx_api_key
OKX_SECRET_KEY=your_okx_secret_key
OKX_PASSPHRASE=your_okx_passphrase
